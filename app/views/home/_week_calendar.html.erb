    <div data-day="tuesday" class="calendar-dayContainer calendar-closed">
      <h6>Tues</h6>
      <div class="calendar-timeSheet">

      </div>
    </div>

    <div data-day="webnesday" class="calendar-dayContainer calendar-closed">
      <h6>Wed</h6>
      <div class="calendar-timeSheet">

      </div>
    </div>

    <div data-day="thursday" class="calendar-dayContainer calendar-closed">
      <h6>Thur</h6>
      <div class="calendar-timeSheet">

      </div>
    </div>

    <div data-day="friday" class="calendar-dayContainer calendar-closed">
      <h6>Fri</h6>
      <div class="calendar-timeSheet">

      </div>
    </div>

    <div data-day="saturday" class="calendar-dayContainer calendar-closed">
      <h6>Sat</h6>
      <div class="calendar-timeSheet">

      </div>
    </div>

    <div data-day="sunday" class="calendar-dayContainer calendar-closed">
      <h6>Sun</h6>
      <div class="calendar-timeSheet">

      </div>
    </div>

  </div>

</div>

<%= content_for :scripts do %>
